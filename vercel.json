{
  "version": 2,
    "builds": [
      {
        "src": "bitpoll/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "stat"
      }
    }
    ],
    "routes": [
       {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
      {
        "src": "/(.*)",
        "dest": "bitpoll/wsgi.py"
      }
    ]
}
